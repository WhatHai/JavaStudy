<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TclTransService WSDL service -->

  <service name="TclTransService" provider="java:RPC" style="document" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ebank.tcl.com/app/TclTrans/intf"/>
      <parameter name="wsdlServiceElement" value="TclTransService"/>
      <parameter name="schemaQualified" value="http://ebank.tcl.com/app/TclTrans/bean"/>
      <parameter name="wsdlServicePort" value="TclTransServiceEndpoint"/>
      <parameter name="className" value="com.tcl.ebank.app.TclTrans.intf.TclTransServiceSoapBindingSkeleton"/>
      <parameter name="wsdlPortType" value="TclTransService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <parameter name="allowedMethods" value="*"/>

      <typeMapping
        xmlns:ns="http://ebank.tcl.com/app/TclTrans/bean"
        qname="ns:>REQUEST"
        type="java:com.tcl.ebank.app.TclTrans.bean.REQUEST"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://ebank.tcl.com/app/TclTrans/bean"
        qname="ns:>RESPONSE"
        type="java:com.tcl.ebank.app.TclTrans.bean.RESPONSE"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
